{
    "name": "cortex/foundation",
    "type": "cortex-module",
    "description": "The core foundation of Rinvex Cortex modular application architecture.",
    "keywords": [
        "rinvex",
        "cortex",
        "module",
        "laravel",
        "foundation",
        "architecture",
        "modular",
        "kernel",
        "heart",
        "core"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-foundation/issues",
        "source": "https://github.com/rinvex/cortex-foundation",
        "docs": "https://github.com/rinvex/cortex-foundation/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-foundation/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-snappy": "^0.4.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.0.0",
        "edvinaskrucas/notification": "^5.2.0",
        "fideloper/proxy": "^4.0.0",
        "h4cc/wkhtmltoimage-amd64": "^0.12.0",
        "h4cc/wkhtmltopdf-amd64": "^0.12.0",
        "laravel/framework": "~5.6.0",
        "laravelcollective/html": "~5.6.0",
        "league/fractal": "^0.17.0",
        "lord/laroute": "^2.4.0",
        "mcamara/laravel-localization": "^1.3.0",
        "proengsoft/laravel-jsvalidation": "^2.2.0",
        "rinvex/cacheable": "dev-develop",
        "rinvex/composer": "dev-develop",
        "rinvex/menus": "dev-develop",
        "rinvex/pages": "dev-develop",
        "rinvex/support": "dev-develop",
        "rinvex/widgets": "dev-develop",
        "silber/bouncer": "v1.0.0-rc.1",
        "spatie/laravel-activitylog": "^2.5.0",
        "spatie/laravel-medialibrary": "v7.x-dev",
        "symfony/console": "^4.0.0",
        "symfony/finder": "^4.0.0",
        "symfony/http-kernel": "^4.0.0",
        "torann/geoip": "^1.0.0",
        "vinkla/hashids": "^5.0.0",
        "watson/validating": "^3.1.0",
        "yajra/laravel-datatables-buttons": "^3.2.0",
        "yajra/laravel-datatables-fractal": "^1.1.0",
        "yajra/laravel-datatables-html": "^3.6.0",
        "yajra/laravel-datatables-oracle": "^8.4.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.6.0",
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Foundation\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Foundation\\Providers\\FoundationServiceProvider",
                "Cortex\\Foundation\\Providers\\ConsoleSupportServiceProvider",
                "Cortex\\Foundation\\Overrides\\Lord\\Laroute\\LarouteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
