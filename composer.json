{
    "name": "cortex/foundation",
    "type": "cortex-module",
    "description": "The core foundation of Rinvex Cortex modular application architecture.",
    "keywords": [
        "rinvex",
        "cortex",
        "module",
        "laravel",
        "foundation",
        "architecture",
        "modular",
        "kernel",
        "heart",
        "core"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-foundation/issues",
        "source": "https://github.com/rinvex/cortex-foundation",
        "docs": "https://github.com/rinvex/cortex-foundation/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-foundation/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "rinvex/fort": "dev-develop",
        "laravel/framework": "~5.5.0",
        "watson/validating": "^3.0.1",
        "rinvex/taggable": "dev-develop",
        "spatie/laravel-fractal": "dev-master as 3.2",
        "rinvex/cacheable": "dev-develop",
        "rinvex/composable": "dev-develop",
        "laravelcollective/html": "dev-master as 5.4.20",
        "barryvdh/laravel-snappy": "dev-master as 0.3.3",
        "kalnoy/nestedset": "dev-master as 4.2.7",
        "maatwebsite/excel": "dev-master as 2.1.17",
        "rinvex/attributable": "dev-develop",
        "edvinaskrucas/notification": "^5.1",
        "spatie/laravel-activitylog": "dev-master as 1.10",
        "spatie/laravel-translatable": "dev-master as 1.2.0",
        "mcamara/laravel-localization": "^1.2.0",
        "proengsoft/laravel-jsvalidation": "dev-master",
        "spatie/eloquent-sortable": "dev-master as 3.0.0",
        "spatie/laravel-sluggable": "dev-master as 1.3.0",
        "yajra/laravel-datatables-buttons": "dev-master as 1.3.1",
        "yajra/laravel-datatables-oracle": "dev-master as 7.7.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/laravel-activitylog"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/laravel-fractal"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/laravel-translatable"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/laravel-sluggable"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/eloquent-sortable"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/laravel-nestedset"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/laravel-snappy"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/html"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/laravel-datatables"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/Laravel-Excel"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Omranic/laravel-datatables-buttons"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cortex\\Foundation\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Foundation\\Providers\\FoundationServiceProvider",
                "Cortex\\Foundation\\Providers\\ConsoleSupportServiceProvider",
                "Cortex\\Foundation\\Overrides\\Lord\\Laroute\\LarouteServiceProvider"
            ]
        },
        "cortex-module": {
            "autoinstall": false,
            "icon": {
                "color": "#2196f3",
                "name": "heartbeat",
                "background-size": "cover",
                "background-color": "#e3f2fd",
                "background-position": "center",
                "background-repeat": "no-repeat"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
